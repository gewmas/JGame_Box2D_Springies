<model>
    <name>Example</name>
    <author>Robert Duvall</author>

    <nodes>
        <mass id="m0" x="100" y="100" vx="0" vy="0" mass="5.0"/>
        <mass id="m1" x="300" y="100" vx="0" vy="0" mass="7.0"/>
        <mass id="m2" x="200" y="300" vx="0" vy="0" mass="10.0"/>
    </nodes>

    <links>
        <spring a="m0" b="m1" restlength="150.0" constant="0.05"/>
        <spring a="m0" b="m2" restlength="100.0" constant="0.05"/>
        <spring a="m1" b="m2" restlength="100.0" constant="0.05"/>
    </links>
    
    <centermass magnitude="1000" exponent="2.0"/>
    <gravity direction="90" magnitude="20"/>
    <viscosity magnitude="0.9"/>
    
    <wall id="1" magnitude="2400" exponent="1.0" />
	<wall id="2" magnitude="2500" exponent="1.0" />
	<wall id="3" magnitude="1800" exponent="1.0" />
	<wall id="4" magnitude="2000" exponent="1.0" />
</model>
